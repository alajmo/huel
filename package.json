{
  "name": "huel",
  "version": "1.0.0-alpha.5",
  "description": "A development tool for the web",
  "main": "bin/huel",
  "bin": {
    "huel": "./bin/huel"
  },
  "files": [
    "bin/huel"
  ],
  "contributors": [],
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.7.0"
  },
  "scripts": {
    "start": "huel build --env development --debug -w -p 1337 -t test/examples/app/src/index.html -e test/examples/app/src/index.js -o test/examples/app/dist/",
    "start-prod": "huel build --env production --debug -w -p 1337 -t test/examples/app/src/index.html -e test/examples/app/src/index.js -o test/examples/app/dist/",
    "build": "huel build --env production --debug -t test/examples/app/src/index.html -e test/examples/app/src/index.js -o test/examples/app/dist/",
    "build-dev": "huel build --env development --debug -t test/examples/app/src/index.html -e test/examples/app/src/index.js -o test/examples/app/dist/",
    "eslint-check": "eslint --print-config config/.eslintrc | eslint-config-prettier-check",
    "lint": "huel lint --src src",
    "lint-watch": "huel lint -w",
    "format": "huel format --src src",
    "format-watch": "huel format -w",
    "commitlint": "huel commitmsg",
    "version": "huel changelog && git add CHANGELOG.md && git commit -u \"chore: Update changelog\"",
    "test": "huel test --pjv --strict-version --size --nodecheck --npmcheck --moduleversioncheck --modulenodecheck --extraneous-modules --update-check"
  },
  "author": "Samir Alajmovic <alajmovic.samir@gmail.com> (https://www.alajmovic.com)",
  "bugs": "https://github.com/samiralajmovic/huel-9000/issues",
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-angular": "6.1.3",
    "@commitlint/core": "6.1.3",
    "autoprefixer": "8.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "bytes": "3.0.0",
    "cache-loader": "1.2.2",
    "chalk": "2.3.2",
    "check-dependencies": "1.1.0",
    "chokidar": "2.0.2",
    "clean-webpack-plugin": "0.1.19",
    "commander": "2.15.0",
    "conventional-changelog": "1.1.18",
    "conventional-changelog-angular": "1.6.6",
    "copy-webpack-plugin": "4.5.0",
    "css-loader": "0.28.10",
    "cssnano": "3.10.0",
    "del": "3.0.0",
    "depcheck": "0.6.9",
    "eslint": "4.18.2",
    "eslint-plugin-prettier": "2.6.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "globby": "8.0.1",
    "html-loader": "1.0.0-alpha.0",
    "html-webpack-plugin": "3.0.6",
    "package-json-validator": "0.6.3",
    "pkg-up": "2.0.0",
    "please-upgrade-node": "3.0.1",
    "postcss-loader": "2.1.1",
    "prettier": "1.11.1",
    "read-pkg-up": "3.0.0",
    "semver": "5.5.0",
    "size-limit": "0.16.2",
    "speed-measure-webpack-plugin": "1.1.1",
    "style-loader": "0.20.2",
    "uglifyjs-webpack-plugin": "1.2.2",
    "webpack": "4.1.1",
    "webpack-dev-server": "3.1.0",
    "webpack-jarvis": "0.3.0",
    "webpack-manifest-plugin": "2.0.0-rc.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "2.9.0",
    "husky": "0.15.0-rc.8",
    "npm-check": "5.5.2",
    "pkg-ok": "2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samiralajmovic/huel.git"
  },
  "homepage": "https://github.com/samiralajmovic/huel",
  "keywords": [
    "html",
    "cli",
    "command line tool",
    "lint",
    "format",
    "prettier",
    "webpack",
    "build",
    "dev",
    "environment"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm test",
      "pre-publish": "npm test",
      "commit-msg": "npm run commitlint"
    }
  }
}

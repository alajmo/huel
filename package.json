{
  "name": "hal-9000",
  "version": "0.8.1",
  "description": "A development environment for the web",
  "main": "bin/hal",
  "bin": {
    "hal": "./bin/hal"
  },
  "files": [
    "bin/hal"
  ],
  "contributors": [],
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.7.0"
  },
  "scripts": {
    "start": "hal build -w -t test/example/src/index.html -e test/example/src/index.js -o test/example/dist/ --env prod",
    "build": "hal build --lint test/example/src --format test/example/src -t test/example/src/index.html -e test/example/src/index.js -o test/example/dist/",
    "eslint-check": "eslint --print-config config/.eslintrc | eslint-config-prettier-check",
    "lint": "hal lint --src test/example/src",
    "lint-watch": "hal lint --watch --src test/example/src",
    "format": "hal format --src test/example/src",
    "format-watch": "hal format --watch --src test/example/src",
    "karma": "karma start karma.config.js",
    "test": "hal test --pjv --strict-version --size --nodecheck --npmcheck --moduleversioncheck --extraneous-modules --update-check --verbose",
    "precommit": "npm run format && pkg-ok && pjv package.json && npm test",
    "prepush": "pkg-ok && pjv package.json && npm test",
    "prepublish": "pkg-ok && pjv package.json && npm test",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "author": "Samir Alajmovic <alajmovic.samir@gmail.com> (https://www.alajmovic.com)",
  "bugs": "https://github.com/samiralajmovic/hal-9000/issues",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-prettier": "2.9.0",
    "karma-chrome-launcher": "2.2.0",
    "npm-check": "5.5.2",
    "package-json-validator": "0.6.3",
    "pkg-ok": "2.1.0"
  },
  "dependencies": {
    "@commitlint/cli": "4.3.0",
    "@commitlint/config-angular": "4.3.0",
    "@commitlint/core": "5.2.8",
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "bytes": "3.0.0",
    "chalk": "2.3.2",
    "check-dependencies": "1.1.0",
    "chokidar": "1.7.0",
    "clean-webpack-plugin": "0.1.18",
    "commander": "2.14.1",
    "conventional-changelog": "1.1.18",
    "conventional-changelog-angular": "1.6.6",
    "css-loader": "0.28.10",
    "cssnano": "3.10.0",
    "del": "3.0.0",
    "depcheck": "0.6.9",
    "eslint": "4.18.2",
    "eslint-plugin-prettier": "2.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "globby": "7.1.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "pkg-up": "2.0.0",
    "please-upgrade-node": "3.0.1",
    "postcss-loader": "2.1.1",
    "prettier": "1.11.1",
    "read-pkg-up": "3.0.0",
    "semver": "5.5.0",
    "size-limit": "0.15.2",
    "style-loader": "0.20.2",
    "tap": "11.1.1",
    "uglifyjs-webpack-plugin": "1.2.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-jarvis": "0.3.0",
    "webpack-monitor": "1.0.14",
    "whatwg-fetch": "2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samiralajmovic/hal.git"
  },
  "homepage": "https://github.com/samiralajmovic/hal",
  "keywords": [
    "html",
    "cli",
    "command line tool",
    "lint",
    "format",
    "prettier",
    "webpack",
    "build",
    "dev",
    "environment"
  ],
  "ava": {
    "require": "babel-register",
    "babel": {
      "extends": "./.babelrc"
    }
  }
}

{
  "name": "example",
  "version": "0.1.0",
  "description": "Example app",
  "scripts": {
    "start": "hal build -w -t src/index.html -e src/index.js -o dist/ --env prod",
    "build": "hal build --lint src --format src -t src/index.html -e src/index.js -o dist/",
    "lint": "hal lint --src src",
    "format": "hal format --src src",
    "test": "node test/test.js",
    "prepush": "npm test",
    "commitmsg": "hal commitmsg",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "npm test"
  },
  "author": "McExample ExampleFace  <example.face@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "hal-9000": "*"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git://github.com/samiralajmovic/hal.git"
  },
  "homepage": "https://github.com/samiralajmovic/hal",
  "ava": {
    "require": "babel-register",
    "babel": {
      "extends": "./.babelrc"
    }
  }
}